#include<iostream>
using namespace std;
class biggest
{
   private:
int a,b,c;
public:
void input();
        void display();
};
void biggest::input()
{
cout<<"Enter 3 nos.:";
cin>>a>>b>>c;
}
void biggest::display()
{
if(a>=b && a >= c)
{
cout<<"Biggest no.:"<<a;}
if(b>=a && b>=c)
{
cout<<"Biggest no.:"<<b;}
if(c>=a && 
c>=b)
{
cout<<"Biggest no.:"<<c;}
}
int main()
{
biggest obj;
obj.input();
obj.display();
}
